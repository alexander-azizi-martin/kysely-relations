{
  "name": "kysely-relations",
  "version": "0.0.5",
  "description": "Kysely relations builder similar to drizzle's relation API",
  "license": "MIT",
  "author": "alexander-azizi-martin",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexander-azizi-martin/kysely-relations"
  },
  "keywords": [
    "kysely",
    "relations",
    "orm"
  ],
  "scripts": {
    "test": "tsx --test test/**/*.test.ts"
  },
  "exports": {
    "./postgres": {
      "types": "./src/postgres/relations-builder.d.ts",
      "import": "./src/postgres/relations-builder.js"
    },
    "./mysql": {
      "types": "./src/mysql/relations-builder.d.ts",
      "import": "./src/mysql/relations-builder.js"
    },
    "./sqlite": {
      "types": "./src/sqlite/relations-builder.d.ts",
      "import": "./src/sqlite/relations-builder.js"
    }
  },
  "files": [
    "src"
  ],
  "main": "./src/postgres/relations-builder.js",
  "types": "./src/postgres/relations-builder.d.ts",
  "peerDependencies": {
    "kysely": ">=0.27.0"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.14",
    "@types/node": "^25.0.3",
    "kysely": "^0.27.0",
    "kysely-pglite-dialect": "^1.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
